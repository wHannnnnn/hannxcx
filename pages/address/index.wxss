.address .content{
    height: calc(100vh);
    background: #fff;
}
.address .addressList {
  border-bottom: 1rpx solid rgba(150, 150, 150, 0.534);
  padding: 30rpx 0rpx;
}
.address .addressList .van-icon {
  font-size: 36rpx
}
.address .van-cell {
  box-sizing: border-box
}
.address .van-cell:after {
  content: none
}
.address .content .edit{
    font-size: 40rpx
}
.address .content .van-tag {
    font-size: 20rpx;
    margin-left: 20rpx;
    padding: 2rpx 6rpx
}
.address .addAddress {
    padding: 0 20rpx;
    box-sizing: border-box;
    position: fixed;
    bottom: 20rpx;
    left: 0;
    width: 100%;
}
.address .addAddress .weui-btn {
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    border-radius: 30rpx;
    background: #e7352c;
    color: #fff;
    font-size: 34rpx;
    box-sizing: border-box
}